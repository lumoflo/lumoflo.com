{
  "name": "@lumoflo/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate",
    "db:migrate-prod": "pnpm with-env prisma migrate deploy",
    "db:migrate": "doppler run -- prisma migrate dev",
    "db:push": "doppler run -- prisma db push",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@lumoflo/tailwind-config": "workspace:*",
    "@lumoflo/ui": "workspace:*",
    "@lumoflo/utils": "workspace:*",
    "@mermaid-js/mermaid-cli": "^10.6.1",
    "@prisma/client": "^5.6.0",
    "prisma-erd-generator": "^1.11.1",
    "supabase": "^1.88.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.16.5",
    "dotenv-cli": "^7.2.1",
    "prisma": "^5.6.0",
    "typescript": "^5.0.4",
    "zod-prisma": "^0.5.4"
  }
}
